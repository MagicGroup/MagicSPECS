%defattr(-,root,root)

%install

make install-strip DESTDIR=$RPM_BUILD_ROOT
install -d -m755 $RPM_BUILD_ROOT/usr/share/locale/zh_CN/LC_MESSAGES
msgfmt /home/magic/rpmbuild/SOURCES/krename.zh_CN.po -o $RPM_BUILD_ROOT/usr/share/locale/zh_CN/LC_MESSAGES/krename.mo

cd $RPM_BUILD_ROOT
find . -type d | sed '1,2d;s,^\.,\%attr(-\,root\,root) \%dir ,' > $RPM_BUILD_DIR/master.list
find . -type f | sed 's,^\.,\%attr(-\,root\,root) ,' >>  $RPM_BUILD_DIR/master.list
find . -type l | sed 's,^\.,\%attr(-\,root\,root) ,' >>  $RPM_BUILD_DIR/master.list

%clean
/usr/bin/rm -rf /home/magic/rpmbuild/BUILDROOT/krename-3.0.14-1mgc30.x86_64 /home/magic/rpmbuild/BUILD/krename-3.0.14 /home/magic/rpmbuild/BUILD/master.list
