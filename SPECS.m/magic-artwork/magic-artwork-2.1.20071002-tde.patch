+		mkdir "$apidoxdir" > /dev/null 2>&1
+	fi
+	cd "$apidoxdir" > /dev/null 2>&1 || { 
+		echo "Cannot create and cd into $apidoxdir"
+		exit 1
+	}
+
+	test -f "Doxyfile.in" || create_doxyfile_in
+
+	# Copy in logos and the like
+	for i in "favicon.ico" "kde_gear_64.png"
