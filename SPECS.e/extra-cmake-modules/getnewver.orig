#!/bin/bash
url="http://mirrors.ustc.edu.cn/kde/stable/frameworks//?C=M&O=D"
downcommand="wget -q" 
pushd $(dirname $0) > /dev/null
rm -f index.html && $downcommand "$url" -O index.html
if [ -f index.html ];then
	newver=`grep "5\.[0-9}\+\.[0-9]\+" index.html -o |head -n 1`
	if [ x$(echo $newver|cut -f3 -d'.') == x"" ]; then
		echo $newver.0
	else
		echo $newver
	fi
else
	echo "不能正常取得新版本号"
	exit 1
fi
rm -f index.html
popd > /dev/null
