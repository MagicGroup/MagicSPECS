    <Filename>xscreensaver-properties.desktop</Filename>
  </Exclude>
  <Menu>
    <Name>Look and Feel</Name>
    <Exclude>
      <Filename>xscreensaver-properties.desktop</Filename>
    </Exclude>
  </Menu>
</Menu>
