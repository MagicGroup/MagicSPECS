+    rmdir /var/lib/wdm/authdir || true
+    rmdir /var/lib/wdm || true
+    update-rc.d wdm remove >/dev/null
+  ;;
+  upgrade|failed-upgrade)
+  ;;
+  abort-install)
+  ;;
+  abort-upgrade)
+  ;;
+  disappear)
+  ;;
+esac
+
+exit 0
